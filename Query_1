class Listops():
    def __init__(self,l):
        self.l = l
    def extract_even(self):
        l = self.l
        l1 = []
        for i in l:
            if i % 2 == 0:
                l1.append(i)
        return l1
    def extract_odd(self):
        l = self.l
        l1 = []
        for i in l:
            if i % 2 != 0:
                l1.append(i)
        return l1
ops1 = Listops([1, 2, 3, 4, 5, 6])
print(ops1.extract_even())
